!function(){var e=document.referrer;0!==e.length&&(isInternalReferrer(e)||(Cookies.set("current_referrer",e,{expires:3650}),void 0===Cookies.get("initial_referrer")&&Cookies.set("initial_referrer",e,{expires:3650})))}(),function(){function e(e){var r=RegExp("[?&]"+e+"=([^&]*)").exec(window.location.search);return r&&decodeURIComponent(r[1].replace(/\+/g," "))}var r=e("gclid");if(r){var t=e("gclsrc");t&&-1===t.indexOf("aw")||Cookies.set("gclid",r,{expires:90})}for(var i=["utm_source","utm_medium","utm_term","utm_content","utm_campaign"],o=0;o<i.length;o++){var n=i[o],a=e(n);n&&a&&Cookies.set(n,a,{expires:90})}}(),$(document).ready(function(){var e=Cookies.get("gclid");e&&$("input.form-adwords-id").val(e),$("input.form-lead-url").val(document.location.pathname);for(var r=["utm_source","utm_medium","utm_term","utm_content","utm_campaign","initial_referrer","current_referrer"],t=0;t<r.length;t++){var i=r[t],o=Cookies.get(i);if(o){var n=$("input.form-"+i.replace("_","-"));n.val(o)}}});